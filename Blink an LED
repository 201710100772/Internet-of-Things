//Blinking of an LED
int Ledpin=12;

void setup()
{
  pinMode(Ledpin, OUTPUT);
  Serial.begin(9600);
}

void loop()
{
  //LED ON
  digitalWrite(Ledpin, HIGH);
  Serial.println("LED ON");
  delay(500); // Wait for 1000 millisecond(s)
   //LED OFF
  digitalWrite(Ledpin, LOW);
  Serial.println("LED OFF");
  delay(500); // Wait for 1000 millisecond(s)
  
}
